[flake8]
docstring-convention=all
import-order-style=pycharm
max_doc_length=72
ignore=
    B311,W503,E226,S311,T000
    # Missing Docstrings
    D100,D104,D105,D107,
    # Docstring Whitespace
    D203,D212,D213
    # More Docstring Whitespace (currently disabled)
    #D214,D215,
    # Docstring Quotes (currently disabled)
    ; D301,D302,
    # Docstring Content
    D413,D407
    # Docstring Content (currently disabled)
    ; D400,D401,D402,D404,D405,D406,D408,D409,D410,D411,D412,D414,D416,D417
    # Type Annotations
    ANN101,ANN102
    # Type Annotations (currently disabled)
    ; ANN002,ANN003,ANN101,ANN102,ANN204,ANN206
